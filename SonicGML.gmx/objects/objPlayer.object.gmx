        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Movement

if (left) {
    fleft = true;
    if (xsp &gt; 0) {
        xsp -= dec;
    } else if (xsp &gt; -top) {
        xsp -= acc;
    }
} else if (right) {
    fleft = false;
    if (xsp &lt; 0) {
        xsp += dec;
    } else if (xsp &lt; top) {
        xsp += acc;
    }
} else {
    xsp -= min(abs(xsp), frc) * sign(xsp);
}

// Gravity and Jumping

ysp += grv;
if (ysp &gt; 16) { ysp = 16; }

if (sensorA || sensorB) {
    if (jump) {
        ysp = -jmp;
    }
} else {
    if (jrel &amp;&amp; ysp &lt; -4) {
        ysp = -4;
    }
}

// Applying the XSP and YSP to the X and Y of the charracter respectively

x += xsp;
y += ysp;
</string>
