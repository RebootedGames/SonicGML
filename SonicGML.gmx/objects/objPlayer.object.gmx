                sprite_index = spritepush;
                image_index = 0;
            }
        }
    }
} else if (abs(xsp) &lt; top &amp;&amp; (center || sensorA || sensorB)) {
    if (spritewalk != undefined &amp;&amp; sprite_index != spritewalk) {
        sprite_index = spritewalk;
        image_index  = 0;
    }
} else if (spriterun != undefined &amp;&amp; sprite_index != spriterun &amp;&amp; center) {
    sprite_index = spriterun;
    image_index  = 0;
} else if (jump &amp;&amp; spritejump != undefined &amp;&amp; sprite_index != spritejump) {
