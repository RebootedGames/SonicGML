    if (!center &amp;&amp; tilecollision(x, y+i)) { center = true; } 
    else if (!center &amp;&amp; position_meeting(x, y+i, objBridge)) { with(instance_place(x, y+i, objBridge)) { 
    objSonic.step = current+1; } center = true; } else { center = false; } // Sensor C
}
